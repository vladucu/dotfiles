{{ if eq .chezmoi.os "darwin" -}}
#!/usr/bin/env zsh

# Uncomment to profile startup
# zmodload zsh/zprof

{{ include "shell/init.zsh" }}

{{ if .use_nix }}
# Nix multi-user installer will add this to the system /etc/zshrc but that is
# overwritten with every system update
#source '/nix/var/nix/profiles/default/etc/profile.d/nix-daemon.sh'
