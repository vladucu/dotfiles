{{  if lookPath "sheldon" }}
# Source sheldon plugins
{{    output "sheldon" "source" | trim }}
{{  end -}}

{{  if lookPath "thefuck" }}
# Fuck
# https://github.com/nvbn/thefuck
{{    output "thefuck" "--alias" | trim }}
{{  end  -}}

{{ end }}